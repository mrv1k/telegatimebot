{
  "name": "telegatimebot",
  "version": "1.0.0",
  "description": "Solves YouTube in Telegram annoyances by getting video duration and converting timestamp.",
  "main": "src/bot.ts",
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "scripts": {
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist",
    "ts-node": "ts-node --files ./src/bot.ts",
    "dev": "NODE_ENV=development npm run ts-node",
    "debug": "NODE_ENV=debug npm run ts-node",
    "dev-with-prod-env": "NODE_ENV=production npm run ts-node",
    "build": "NODE_ENV=production tsc -p tsconfig.json",
    "start:pm2": "pm2-runtime start ecosystem.config.js --env production",
    "ship-it": "npm run clean && npm run build && npm run start:pm2",
    "start": "npm install && npm run ship-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrv1k/telegatimebot.git"
  },
  "keywords": [
    "telegram",
    "youtube",
    "bot"
  ],
  "author": "Viktor Khotimchenko <viktorkhotimchenko@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mrv1k/telegatimebot/issues"
  },
  "homepage": "https://github.com/mrv1k/telegatimebot#readme",
  "dependencies": {
    "@googleapis/youtube": "^2.0.0",
    "dotenv": "^10.0.0",
    "ioredis": "^4.27.2",
    "iso8601-duration": "^1.3.0",
    "js-video-url-parser": "^0.4.3",
    "pm2": "^5.1.0",
    "telegraf": "^4.3.0"
  },
  "devDependencies": {
    "@types/ioredis": "^4.26.3",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.2.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  }
}
